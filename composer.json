{
  "name": "yuyangame/yii2-qiniu-sdk",
  "type": "yii2-extension",
  "description": "Qiniu API for Yii 2.0",
  "keywords": ["yii2", "qiniu", "upload", "sdk"],
  "homepage": "https://github.com/yuyangame/yii2-qiniu-sdk",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/chocoboxxf/yii2-qiniu-sdk/issues",
    "wiki": "https://github.com/chocoboxxf/yii2-qiniu-sdk/wiki",
    "source": "https://github.com/yuyangame/yii2-qiniu-sdk"
  },
  "authors": [
    {
      "name": "yuyangame",
      "email": "kangzhq@foxmail.com",
      "homepage": "https://github.com/yuyangame",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "qiniu/php-sdk": "v7.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "yuyangame\\Qiniu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yuyangame\\Qiniu\\Tests\\": "tests/"
    }
  }
}